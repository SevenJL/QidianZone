<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zone.mapper.UserMapper">
    <insert id="insertUser">
        INSERT INTO user(name,password,email,create_time,update_time,status,nick_name,power,avatar_url)
        VALUES (#{name},#{password},#{email},#{createTime},#{updateTime},#{status},#{nickName},#{power},#{avatarUrl})
    </insert>
</mapper>